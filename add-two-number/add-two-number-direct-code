// 
OUTPUT: 
a=10
b=20
the sum of numbers are= 30

//


Code-->



public class twonumbers {
	
	public static void main(String[] args) {
		int a=10;
		int b=20;
		int sum;


		sum=a+b;
		System.out.println("a="+a);
		System.out.println("b="+b);
		System.out.println("the sum of numbers are= " +sum);
	
	}

}
